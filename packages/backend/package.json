{
  "name": "freeformapi-backend",
  "version": "1.2.0",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "dev:refactor": "nodemon --exec \"ts-node src/index.ts\"",
    "start": "node dist/index.js",
    "build": "tsc",
    "test": "jest --passWithNoTests",
    "test:unit": "jest tests/unit --passWithNoTests",
    "test:integration": "jest tests/integration --passWithNoTests",
    "test:e2e": "jest tests/e2e --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:ci": "jest --ci --coverage --passWithNoTests",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "refactor:day1": "npm run type-check && npm run lint && npm run dev:refactor",
    "backup": "cp src/index.ts src/index.backup.ts",
    "restore": "cp src/index.backup.ts src/index.ts",
    "db:test:setup": "node tests/utils/setup-test-db.js"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "dotenv": "^16.4.1",
    "fastify": "^4.26.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.7.0",
    "jest-html-reporter": "^4.3.0",
    "nodemon": "^3.0.2",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.1.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
