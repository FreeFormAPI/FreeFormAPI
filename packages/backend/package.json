{
  "name": "freeformapi-backend",
  "version": "1.2.0",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "dev:refactor": "nodemon --exec \"ts-node src/index.ts\"",
    "start": "node dist/index.js",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "refactor:day1": "npm run type-check && npm run lint && npm run dev:refactor",
    "backup": "cp src/index.ts src/index.backup.ts",
    "restore": "cp src/index.backup.ts src/index.ts"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "dotenv": "^16.4.1",
    "fastify": "^4.26.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/ioredis": "^5.0.0",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "nodemon": "^3.0.2",
    "pino-pretty": "^13.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
